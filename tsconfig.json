{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "./src",
    "paths": {"date-fns/format": ["node_modules/date-fns/format"] ,
      "@/*": [
        "./*"
      ]
    }
